msgstr "Kumotut"

#: View/Atlas/index.ctp:836
msgid "Outdated"
msgstr "Vanhentuneet"

#: View/Atlas/index.ctp:839
msgid "Cluster"
msgstr "Rypäs"

#: View/Atlas/index.ctp:840
msgid "Frequent Spawn Area"
msgstr "Yleinen esiintymisalue"

#: View/Atlas/index.ctp:841
msgid "Frequent Spawn Point"
msgstr "Yleinen esiintymispiste"

#: View/Atlas/index.ctp:842
